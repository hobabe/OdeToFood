@page
@model OdeToFood.Pages.Restaurents.ListModel
@{
}
<h1>Restaurants</h1>
<table class="table">
    @foreach (var restaurant in Model.Restaurants)
    {
        <tr>
            <td>@restaurant.Name</td>
            <td>@restaurant.Location</td>
            <td>@restaurant.Cuisine</td>
        </tr>
    }
</table>

<hr />
@Model.Message
<br />
@Model.MessageFromAppsettings
